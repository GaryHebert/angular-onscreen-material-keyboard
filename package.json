    "@angular-eslint/template-parser": "2.0.2",
    "@angular/cli": "11.2.6",
    "@angular/compiler-cli": "11.2.7",
    "@angular/language-service": "11.2.7",
    "@compodoc/compodoc": "^1.1.11",
    "@types/jasmine": "~3.6.9",
    "@types/node": "^14.14.37",
    "@typescript-eslint/eslint-plugin": "4.19.0",
    "@typescript-eslint/parser": "4.19.0",
    "codelyzer": "^6.0.1",
    "cpx": "^1.2.1",
